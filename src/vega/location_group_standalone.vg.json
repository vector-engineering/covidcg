      ],
      "on": [
        { "trigger": "clearGroup", "remove": true },
        { "trigger": "!shiftGroup", "remove": true },
        { "trigger": "!shiftGroup && clickedGroups", "insert": "clickedGroups" },
        { "trigger": "shiftGroup && clickedGroups", "toggle": "clickedGroups" }
      ]
    },
    {
      "name": "selectedLocations",

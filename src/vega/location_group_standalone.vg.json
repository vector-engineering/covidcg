        { "trigger": "clearLocation", "remove": true },
        { "trigger": "!shiftLocation", "remove": true },
        { "trigger": "!shiftLocation && clickedLocations", "insert": "clickedLocations" },
        { "trigger": "shiftLocation && clickedLocations", "toggle": "clickedLocations" }
      ]
    },
